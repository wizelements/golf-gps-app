{
  "name": "golf-gps-tracker",
  "version": "1.0.0",
  "description": "Offline golf GPS and shot tracking app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cp dist/index.html dist/404.html",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "cap:init": "npx cap init 'Golf GPS' com.golfgps.app --web-dir dist",
    "cap:add": "npx cap add android",
    "cap:sync": "npm run build && npx cap sync",
    "cap:open": "npx cap open android"
  },
  "keywords": [
    "golf",
    "gps",
    "offline",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dexie": "^4.0.10",
    "leaflet": "^1.9.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "uuid": "^11.0.4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
    "@capacitor/geolocation": "^6.1.1",
    "@types/leaflet": "^1.9.15",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-window": "^7.3.0"
  }
}
